<%= form_for(:photos, :url => Photo, :html => {:multipart => true, :id => 'photos_upload_form'}) do |f| %>
  <%= content_tag :label, photos_path, :class => 'btn', 'data-disable-with' => 'Uploading...', 'data-behaviour' => 'submit-on-set' do %>
    <%= file_field_tag "file[]", :multiple => true %> Upload Photos
  <% end %>
  <%= link_to 'View the gallery', photos_path, :class => 'btn' %>
<% end %>
